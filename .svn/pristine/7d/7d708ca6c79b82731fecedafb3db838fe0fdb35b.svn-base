<ion-header>

    <ion-navbar>
        <ion-title>QRCode</ion-title>
    </ion-navbar>

</ion-header>


<ion-content padding>

    <ion-card>
        回调结果: {{"code = " + this.xviewResult.code + "--message = " + this.xviewResult.message }}
    </ion-card>

  <ion-card-content>
    <img [src]="qrImage.imagePrefix + qrImage.imageBase64">
  </ion-card-content>

  <button ion-item (click)="scanCode()">扫码</button>
  <button ion-item (click)="setImage()">解析二维码</button>
  <button ion-item (click)="qrCreateQRCode()">生成二维码</button>
  <button ion-item (click)="qrCreateBarCode()">生成条形码</button>
</ion-content>


