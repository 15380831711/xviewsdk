    <button ion-item (click)="Exit()">退出app 杀死 app</button>
    <button ion-item (click)="Hide()">app 只是退到后台 常用退出方式</button>
    <button ion-item (click)="appOnBack()">Android返回键 注册监听</button>


    <button ion-item (click)="xviewUpdateApp()">版本更新</button>
    <button ion-item (click)="updateAppVersionName()">获取版本名称</button>
